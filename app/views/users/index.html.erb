<div class="topbar clearfix">
  <%= link_to_function "Back", "history.back()", :class=>"topbar-btn topbar-back fleft" %>
  <span class="toplogo"><%= link_to 'Qwest', root_path %></span>
</div>

<ul class="quest-list">
  <% @top_users.each do |user| %>
    <li>
      <%= link_to user_fb_picture(user, 'square', :class => ' avatar fleft'), user %>
      <%= user.username %><br/>
      <%= user.cp %> CPs<br/>
      <%= user.quests.count %> quests created<br/>
      <%= user.quest_entries.status_won.count %> quests won<br/>
      <%= user.quest_entries.status_not_won.count %> quests participated<br/>
    </li>
  <% end %>
</ul>
